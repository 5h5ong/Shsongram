{
  "name": "shsongram",
  "version": "1.0.0",
  "description": "Instragram clone with Express + Prisma + React and React Native",
  "repository": "https://github.com/5h5ong/shsongram",
  "author": "5h5ong <Call5h5ong@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/runtime": "^7.5.5",
    "copy": "^0.3.2",
    "dotenv": "^8.0.0",
    "graphql-tools": "^4.0.5",
    "graphql-yoga": "^1.18.0",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.5.8",
    "morgan": "^1.9.1",
    "nodemailer": "^6.2.1",
    "nodemailer-mailgun-transport": "^1.4.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "prisma": "^1.34.5",
    "prisma-client-lib": "^1.34.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.5.5",
    "nodemon": "^1.19.1"
  },
  "scripts": {
    "dev": "nodemon --exec babel-node src/server.js",
    "deploy": "prisma deploy",
    "generate": "prisma generate",
    "prisma": "yarn run deploy && yarn run generate",
    "prebuild": "yarn run prisma",
    "build": "babel src -d build",
    "postbuild": "cd src && copy ./api/**/*.graphql ../build/api",
    "start": "node build/server.js"
  }
}
